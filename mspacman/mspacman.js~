function draw(){
  x = document.getElementById('canvas').getContext('2d');
  y = document.getElementById('canvas').getContext('2d');
  z = document.getElementById('canvas').getContext('2d');

  x.drawImage(document.getElementById('source'), 318, 1, 467, 137, 1, 1, 467, 137);
  y.drawImage(document.getElementById('source'), 79, 1, 20, 18, 278, 52, 20, 18);
  z.drawImage(document.getElementById('source'), 1, 80, 16, 20, 76, 38, 16, 20);

}
